<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job instance="SoftLoft\ConsentManagementSystem\Cron\UpdateVersionConsent" method="execute" name="update_version_consent">
            <schedule>0 1 * * *</schedule>
        </job>
        <job instance="SoftLoft\ConsentManagementSystem\Cron\SendConsentMessageQueue" method="execute" name="send_consent_message_queue">
            <schedule>0 * * * *</schedule>
        </job>
    </group>
</config>
